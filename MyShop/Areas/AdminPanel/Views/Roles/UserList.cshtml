@model IEnumerable<MyShop.Models.User>
 
<h2>Список пользователей</h2>
<table class="table">
    <tr>
        <th>Имя</th>
        <th>Фамилия</th>
        <th>Email</th>
        <th></th>
    </tr>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
            <td>@user.Email</td>
            <td>
                <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Права доступа</a>
            </td>
        </tr>
    }
</table>